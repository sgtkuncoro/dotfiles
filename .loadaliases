#!/bin/sh

# Add Allias Here

# NVIM
alias vim=nvim
alias vi=nvim

## Python
alias python="python3"
alias pip="pip3"

## Command
alias cl="clear"
alias rs="reset"
alias nf="neofetch"

## NodeJs Script Start
### YARN
alias ys="y start"
alias yd="y dev"
alias yt="y test"
alias ytd="y test:dev"
alias ytw="y test:watch"
alias yw="y workspace"

### NPM 
alias n="npm"
alias nr="npm run"
alias ns="npm run start"
alias nd="npm run dev"
alias nt="npm run test"
alias ntd="npm run test:dev"
alias ntw="npm run test:watch"

### PNPM
alias pns="pnpm run start"
alias pnd="pnpm run dev"
alias pnt="pnpm run test"
alias pntd="pnpm run test:dev"
alias pntd="pnpm run test:watch"
alias pn="pnpm"
alias pnr="pnpm run"
alias nvmu="nvm use"

## NodeJS Script End 

## Switch DefaultBrowser 
### To avoid warn dialog confirmation switching
alias edgemac='osascript /Users/$USER/.script/setdefaultbrowser.scpt edgemac'
alias chrome='osascript /Users/$USER/.script/setdefaultbrowser.scpt chrome'



