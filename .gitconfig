[user]
	name = Sigit Kuncoro
	email = sigit.kunc@gmail.com

# Git LinkAja
[includeIf "gitdir:~/WorkSpace/linkaja/"]
	path = "~/WorkSpace/linkaja/.gitconfig"

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
	excludesfile = /Users/kunc/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/kunc/.stCommitMsg
[credential]
	helper = cache
